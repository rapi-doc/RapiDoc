{
  "name": "rapidoc",
  "version": "9.3.9-beta",
  "description": "RapiDoc - Open API spec viewer with built in console",
  "author": "Mrinmoy Majumdar <mrin9@yahoo.com>",
  "homepage": "https://rapidocweb.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/rapi-doc/RapiDoc.git"
  },
  "license": "MIT",
  "keywords": [
    "OpenAPI",
    "OpenAPI Specification",
    "Swagger",
    "JSON-Schema",
    "API",
    "REST",
    "documentation",
    "Custom Element",
    "Web Component",
    "rapidoc",
    "swagger",
    "swagger ui",
    "openapi ui",
    "openapi themes",
    "openapi viewer",
    "openapi renderer",
    "openapi explorer",
    "swagger themes"
  ],
  "type": "module",
  "module": "dist/rapidoc-min.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.js'",
    "format": "prettier --config .prettierrc --check './src/**/*.{js,html,css}'",
    "format-fix": "prettier --config .prettierrc --write './src/**/*.{js,html,css}'",
    "analyze": "lit-analyzer src"
  },
  "dependencies": {
    "@apitools/openapi-parser": "0.0.33",
    "base64-arraybuffer": "^1.0.2",
    "buffer": "^6.0.3",
    "lit": "^3.2.1",
    "marked": "4.3.0",
    "prismjs": "^1.29.0",
    "randexp": "^0.5.3",
    "xml-but-prettier": "^1.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "vite": "^5.4.9",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-list-directory-contents": "^1.4.5"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
