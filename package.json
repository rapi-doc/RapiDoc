  "main": "dist/rapidoc-min.js",
  "module": "dist/rapidoc-min.js",
  "mode": "production",
  "dependencies": {
    "@apitools/openapi-parser": "^0.0.25",
    "babel-plugin-prismjs": "^2.1.0",
    "base64-arraybuffer": "^1.0.2",
